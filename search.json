---
layout: none
---
[
  {% for post in site.posts %}
    {
      "title": "{{ post.title | escape }}",
      "url": "{{ post.url | escape }}",
      "date": "{{ post.date | date_to_string }}",
      "content": "{{ post.content | strip_html | truncatewords: 50 }}"
    }{% unless forloop.last %},{% endunless %}
  {% endfor %}
]
